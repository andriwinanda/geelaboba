    "material-icons": "^1.9.1",
    "skeleton-elements": "^3.5.0",
    "swiper": "^6.8.4",
    "vue": "^3.2.20",
    "vue-numeric": "^2.4.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/compiler-sfc": "^3.2.20",
    "cpy-cli": "^3.1.1",
